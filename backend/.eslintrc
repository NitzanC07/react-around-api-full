{
  "extends": "airbnb-base",
    "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-sequences": ["error", { "allowInParentheses": true }],
    "no-unused-expressions": ["error", { "allowShortCircuit": true, "allowTernary": true }]
  }
}
